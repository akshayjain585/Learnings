public class FirstLastOccurrenceInBinarySearch {
	
	public int firstLastOccurrenceBS(int[] list, int key){
		int start = 0;
		int end = list.length-1;
		int result = -1;   //Return -1 if we don't find element in an array
		while(start<=end){
			int mid = (start+end)/2;
			if(key == list[mid]){
				
			}
		}
		
		return -1;
	}

